        <Button x:Name="btnEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="218,214,0,0" VerticalAlignment="Top" Width="75" Height="29" Click="btnEliminar_Click"/>
        <Button x:Name="btnSalir" Content="Salir" HorizontalAlignment="Left" Margin="115,214,0,0" VerticalAlignment="Top" Width="75" Height="29" Click="btnSalir_Click"/>

    </Grid>
</mah:MetroWindow>

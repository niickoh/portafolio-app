        <Label x:Name="lblInstrucciones" Content="Seleccione un ID para eliminar..." HorizontalAlignment="Left" Margin="43,79,0,0" VerticalAlignment="Top" Width="187"/>
        <Label x:Name="lblEliminarID" Content="ID" HorizontalAlignment="Left" Margin="43,135,0,0" VerticalAlignment="Top" Width="95"/>
